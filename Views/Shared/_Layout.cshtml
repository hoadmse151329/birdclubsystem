                        </div>
                    </div>
                    <div class="GuestButton" style="width: 128px; height: 172px; padding-bottom: 112px; padding-left: 34px; padding-right: 34px; left: 593px; top: 0px; position: absolute; flex-direction: column; justify-content: flex-start; align-items: center; display: inline-flex">
                        <div class="GuestButton" style="width: 60px; height: 60px; position: relative; flex-direction: column; justify-content: flex-start; align-items: flex-start; display: flex">
                            <div class="Ellipse3" style="width: 60px; height: 60px; background: rgba(74.29, 195.50, 0, 0.35)"></div>
                            <div class="IcBaselinePersonOutline" style="width: 40px; height: 40px; padding: 6.67px; justify-content: center; align-items: center; display: inline-flex">
                                <div class="Vector" style="width: 26.67px; height: 26.67px; background: black"></div>
                            </div>
                        </div>
                    </div>
                    <div class="Home" style="left: 0px; top: 22px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Home</div>
                    <div class="News" style="left: 70px; top: 22px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">News</div>
                </div>
                <img class="RedWhiskeredBulbulBirdLogoEps10Generated1" style="width: 70px; height: 86px; left: 5px; top: 9px; position: absolute" src="/images/Red_whiskered_bulbul_bird_logo_eps_10_generated 1.png" />
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - birdclubsystem - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
