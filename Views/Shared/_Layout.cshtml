        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3" style="backdrop-filter:blur() background: rgba(255, 255, 255, 0.8)">
            <div class="container-fluid">
                <div class="ChaomaoClub" style="width: 162px; height: 35px; left: 54px; top: 17px; position: absolute; text-align: center; color: #1F5300; font-size: 30px; font-family: Inter; font-weight: 700; word-wrap: break-word">ChaoMao <br />Club</div>
                <div class="Navbar" style="width: 721px; height: 172px; left: 718px; top: 22px; position: absolute">
                    <div class="Meetings" style="left: 336px; top: 22px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Meetings</div>
                    <div class="FieldTrips" style="left: 234px; top: 22px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Field trips</div>
                    <div class="GuestContestsButton" style="width: 128px; height: 82px; padding-bottom: 39px; padding-left: 28px; padding-right: 28px; left: 110px; top: 22px; position: absolute; justify-content: center; align-items: center; display: inline-flex">
                        <div class="News" style="position: relative">
                            <div class="Contests" style="left: 0px; top: 0px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Contests</div>
                            <div class="IcBaselineArrowBackIosNew" style="width: 24px; height: 24px; padding-top: 2px; padding-bottom: 2px; padding-left: 6px; padding-right: 6.23px; left: 24px; top: 43px; position: absolute; transform: rotate(-90deg); transform-origin: 0 0; justify-content: center; align-items: center; display: inline-flex">
                                <div class="Vector" style="width: 11.77px; height: 20px; background: black"></div>
                            </div>
                        </div>
                    </div>
                    <div class="Gallery" style="left: 549px; top: 22px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Gallery</div>
                    <div class="CommunityDropdown" style="width: 91px; height: 43px; left: 434px; top: 22px; position: absolute">
                        <div class="Community" style="left: 0px; top: 0px; position: absolute; text-align: center; color: black; font-size: 16px; font-family: Inter; font-weight: 700; word-wrap: break-word">Community</div>
                        <div class="IcBaselineArrowBackIosNew" style="width: 24px; height: 24px; padding-top: 2px; padding-bottom: 2px; padding-left: 6px; padding-right: 6.23px; left: 34px; top: 43px; position: absolute; transform: rotate(-90deg); transform-origin: 0 0; justify-content: center; align-items: center; display: inline-flex">
