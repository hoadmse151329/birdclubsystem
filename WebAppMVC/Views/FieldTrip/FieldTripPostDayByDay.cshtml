@{
    ViewData["Title"] = "Field Trip Post Day By Day Page";
}

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/Meeting.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
    <!-- FONT -->
    <link
        href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,300;0,400;0,700;0,900;1,100&family=Roboto+Condensed:wght@400;700&display=swap"
        rel="stylesheet">
    <!-- FONT AWESOME -->
    <link rel="stylesheet" href="~/fonts/fontawesome-free-5.15.4-web/css/all.min.css">

    <!-- main CSS -->
  </head>
  <body>
    <div class="member-meeting-post">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="background">
            <div class="overlap-group">
              <img class="subtract" src="~/images/Background.png" />
            </div>
          </div>
          <div class="headlines">
            <div class="group">
              <div class="overlap-2">
                <div class="flexcontainer">
                  <p class="text">

                    <span class="text-wrapper">Content<br /></span>
                  </p>
                  <p class="span-wrapper">
                    <span class="span">
                        Description
                    </span>
                  </p>
                </div>
                <div class="flexcontainer-2">
                  <p class="text">
                    <span class="text-wrapper">Notes<br /></span>
                  </p>
                  <p class="p">
                    <span class="span">
                        Note
                    </span>
                  </p>
                </div>
                <div class="tittle">
                    <div class="div-wrapper"><div class="text-wrapper-3">Register</div></div>
                  </div>
                <img
                  class="soi-noi-hoi-thi-chim"
                  src="~/images/meeting.png"
                />
              </div>
            </div>
            <div class="info">
              <div class="overlap-3"> 
                <div id="map-container"></div>
                <div class="tittle">
                  <div class="overlap-group-2"><p class="text-wrapper-4">@Model.MeetingName</p></div>
                </div>
                <div class="text-wrapper-5">From StartDate to EndDate</div>


                <div class="group-2">
                  <div class="text-wrapper-6">Participation Limit</div>
                  <div class="text-wrapper-7">Registration Deadlines</div>
                  <div class="text-wrapper-8">Location</div>
                  <div class="text-wrapper-9">Contact</div>
                  <div class="text-wrapper-10">Organizer</div>
                </div>



                <div class="group-3">
                  <div class="text-wrapper-11">AreaNumber,Street,District,City</div>
                  @*<div class="text-wrapper-11">@Model.Street</div>*@
                  <p class="text-wrapper-12">NumberOfParticipantsLimit</p>
                  <div class="text-wrapper-13">RegistrationDeadline</div>
                  <div class="text-wrapper-14">ChaoMaoClub@gmail.com</div>
                  <div class="text-wrapper-15">Host</div>
                </div>
                <div class="register">
                            <div class="overlap-group-3 text-wrapper-16">
                                <span class="text-success">PartNumber</span>
                            </div>
                            <form asp-action="MeetingDeRegister" asp-controller="Meeting" method="post" asp-route-meetingId="MeetingId">
                                <button class="overlap-group-3 text-wrapper-16" type="submit">Sign Out</button>
                            </form> 
                </div>
                <ul class="nav nav-tabs">
                                <!-- dung vong for o day -->
                                <li class="nav-item">
                                    <a class="nav-link active" data-toggle="tab" href="#home1">
                                        <i class='bx bx-home-alt-2'></i>
                                        <span>Home 1</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#home2">
                                        <i class='bx bx-home-alt-2'></i>
                                        <span>Home 2</span>
                                    </a>
                                </li>                
                            </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>