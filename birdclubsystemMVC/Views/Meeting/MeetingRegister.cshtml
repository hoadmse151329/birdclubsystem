@{
    ViewData["Title"] = "Meeting Register Page";
}

<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/Meeting.css" />
  </head>
  <body>
    <div class="meeting-register">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="background">
            <div class="overlap-group">
              <img class="subtract" src="~/images/subtract.png" />
              <div class="rectangle"></div>
              <div class="div"></div>
            </div>
          </div>
          <div class="headlines">
            <div class="overlap-2">
              <div class="info">
                <div class="overlap-3">
                  <div class="rectangle-2"></div>
                  <div class="tittle">
                    <div class="div-wrapper"><div class="text-wrapper">Register</div></div>
                  </div>
                  <div class="login">
                    <div class="overlap-group-2"><div class="text-wrapper-2">Register</div></div>
                  </div>
                  <div class="group">
                    <div class="user-input">
                      <div class="overlap-group-3">
                        <div class="rectangle-3"></div>
                        <div class="text-wrapper-3">FullName</div>
                      </div>
                    </div>
                  </div>
                  <div class="user-input-wrapper">
                    <div class="user-input">
                      <div class="overlap-group-3">
                        <div class="rectangle-3"></div>
                        <div class="text-wrapper-4">Username</div>
                      </div>
                    </div>
                  </div>
                  <div class="group-2">
                    <div class="user-input">
                      <div class="overlap-group-3">
                        <div class="rectangle-3"></div>
                        <div class="text-wrapper-5">Gender</div>
                      </div>
                    </div>
                  </div>
                  <div class="group-3">
                    <div class="user-input">
                      <div class="overlap-group-3">
                        <div class="rectangle-3"></div>
                        <div class="text-wrapper-6">Email</div>
                      </div>
                    </div>
                  </div>
                  <div class="group-4">
                    <div class="user-input">
                      <div class="overlap-group-3">
                        <div class="rectangle-3"></div>
                        <div class="text-wrapper-7">Phone Number</div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="video">
                <div class="button"><img class="bx-play" src="~/images/bx-play.svg" /></div>
              </div>
              <p class="p">Join us for an amazing Bird Lovers Community</p>
            </div>
          </div>
          <p class="chaomao-birdclub">
            <span class="span">ChaoMao </span> <span class="text-wrapper-8">BirdClub</span>
          </p>
        </div>
      </div>
    </div>
  </body>
</html>
